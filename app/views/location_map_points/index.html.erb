<h1>Listing location_map_points</h1>

<table>
  <tr>
    <th>Point name</th>
    <th>Target</th>
    <th>Cox</th>
    <th>Coy</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @location_map_points.each do |location_map_point| %>
  <tr>
    <td><%= location_map_point.point_name %></td>
    <td><%= location_map_point.target_id %></td>
    <td><%= location_map_point.cox %></td>
    <td><%= location_map_point.coy %></td>
    <td><%= link_to 'Show', location_map_point %></td>
    <td><%= link_to 'Edit', edit_location_map_point_path(location_map_point) %></td>
    <td><%= link_to 'Destroy', location_map_point, confirm: 'Are you sure?', method: :delete,:remote=>:true%></td>
  </tr>
<% end %>
</table>

<br />

